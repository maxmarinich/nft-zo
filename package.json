{
  "name": "nft-zo",
  "version": "0.0.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile": "npx hardhat compile",
    "deploy": "rm -rf artifacts cache; npx hardhat run scripts/deploy.js --network rinkeby",
    "mint": "npx hardhat run scripts/mint.js --network rinkeby",
    "burn": "npx hardhat run scripts/burn.js --network rinkeby",
    "transfer": "npx hardhat run scripts/transter.js --network rinkeby",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npx hardhat node",
    "local:deploy": "npx hardhat run scripts/deploy.js --network localhost",
    "local:mint": "npx hardhat run scripts/local/mint.local.js --network localhost",
    "local:transfer": "npx hardhat run scripts/local/transfer.local.js --network localhost",
    "build:client": "rm -rf dist; webpack --mode=production --node-env=production",
    "build:api": "cp -r static/. dist/api",
    "build": "npm run build:client && npm run build:api",
    "serve": "webpack serve"
  },
